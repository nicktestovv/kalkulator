#include "stdafx.h"
#include<iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "RUSSIAN");
	double a, b;
	cout << "Введите вес первого кота: ";
	cin >> a;
	cout << "Введите вес второго кота: ";
	cin >> b;

	if (a > 5) {
		cout << "Первый кот жирный!" << endl;
	}
	else if (b>5) {
		cout << "Второй кот жирный!" << endl;
	} 
	else if ((a > 5) && (b > 5)) {
		cout << "Коты не жирные!" << endl;
	}
	else {
		cout << "Коты не жирные!)" << endl;
	}
	system("pause"); 
    return 0;
}
