#include "stdafx.h"
#include<iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "RUSSIAN");
	int a, b, i, j;
	double p;

	for (i = 5; i <= 5; i++) {
		for (j = 0; j < 10; j++)
		{
			cout << i;
			cout << " * ";
			cout << j;
			p = i*j;
			cout << " = ";
			cout << p << endl;
		}
	}

	system("pause"); 
    return 0;
}
